<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

    <h1>Productos</h1>

    <ul>
        <% productos.forEach(function(producto) { %>
            <li>id: <%= producto.id%>// Nombre:<%= producto.nombre%>// Precio:<%= producto.precio%>// Cantidad: <%= producto.cantidad%></li>
        <% }); %> 
    </ul>

    <hr>

    <h2>Total: <%=productos.length%></h2>

</body>

</html>